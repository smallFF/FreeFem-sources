include "test.idp"

cout << "swap" << endl;
real t;

cout << IND << "real" << endl;

real a = 4.2;
real b = 2.4;
real c = -4.2;
real d = -2.4;

t = clock();
swap(a, b);
t = clock() - t;
cout << IND << IND << t << endl;
test(a == 2.4 && b == 4.2);

t = clock();
swap(c, d);
t = clock() - t;
cout << IND << IND << t << endl;
test(c == -2.4 && d == -4.2);
