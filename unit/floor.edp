include "test.idp"

cout << "floor" << endl;
real t;

cout << IND << "real" << endl;

real r1 = 0.5;
real r2 = 42.0;
real r3 = -1.5;

t = clock();
real resr1 = floor(r1);
t = clock() - t;
cout << IND << IND << t << endl;
test(resr1 == 0);

t = clock();
real resr2 = floor(r2);
t = clock() - t;
cout << IND << IND << t << endl;
test(resr2 == 42);

t = clock();
real resr3 = floor(r3);
t = clock() - t;
cout << IND << IND << t << endl;
test(resr3 == -2);
