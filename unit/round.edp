include "test.idp"

cout << "round" << endl;
real t;

real rpos = 0.5;
real rneg = -0.5;

cout << IND << "real" << endl;

t = clock();
real respos = round(rpos);
t = clock() - t;
cout << IND << IND << t << endl;
test(respos == 1.0);

t = clock();
real resneg = round(rneg);
t = clock() - t;
cout << IND << IND << t << endl;
test(resneg == -1.0);
