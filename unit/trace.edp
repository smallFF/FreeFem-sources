include "test.idp"

cout << "trace" << endl;

real t;

cout << IND << "matrix" << endl;

t = clock();
real Tr = trace([[1, 2, 3],
                 [4 ,5, 6],
                 [7, 8, 9]]);
t = clock() - t;
cout << IND << IND << t << endl;
test(Tr == 15);

t = clock();
Tr = trace([[1, 2, 3],
            [4 , -5, 6],
            [7, 8, 9]]);
t = clock() - t;
cout << IND << IND << t << endl;
test(Tr == 5);

t = clock();
Tr = trace([[-1, 2],
            [-3, 4]]);
t = clock() - t;
cout << IND << IND << t << endl;
test(Tr == 3);

t = clock();
Tr = trace([[-1.5, 2.],
            [-3., 1.5]]);
t = clock() - t;
cout << IND << IND << t << endl;
test(Tr == 0);
