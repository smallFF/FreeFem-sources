include "test.idp"

cout << "emptymesh" << endl;
real t;

mesh Th = square(10, 10);
int[int] ssd(Th.nt);
// Builds the pseudo region numbering
for(int i = 0; i < ssd.n; i++){
   int iq = i/2; // Because we have 2 triangles per quad
   int ix = iq%10;
   int iy = iq/10;
   ssd[i] = 1 + (ix>=5) + (iy>=5)*2;
}

t = clock();
mesh eTh = emptymesh(Th, ssd);
t = clock() - t;
test(Th.nt > eTh.nt);
